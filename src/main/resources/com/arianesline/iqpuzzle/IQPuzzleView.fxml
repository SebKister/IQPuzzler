<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.arianesline.iqpuzzle.IQPuzzleController">
    <ToolBar prefHeight="40.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Button mnemonicParsing="false" onAction="#onSolve" text="Solve" />
      <Button mnemonicParsing="false" onAction="#onStopSolve" text="Stop" />
      <Button mnemonicParsing="false" onAction="#onRefreshUI" text="Refresh" />
      <CheckBox fx:id="UIUpdateCheckBox" mnemonicParsing="false" text="UI Update" />
    </ToolBar>
   <HBox alignment="CENTER" layoutY="39.0" prefHeight="107.0" prefWidth="716.0" spacing="10.0">
      <children>
          <Button mnemonicParsing="false" onAction="#onResetChallenge" text="Reset" />
         <Button mnemonicParsing="false" onAction="#onLoadChallenge" text="Load" />
         <Button mnemonicParsing="false" onAction="#onSaveChallenge" text="Save As" />
         <ComboBox fx:id="partComboBox" onAction="#onPartSelected" prefWidth="150.0" />
       <Button mnemonicParsing="false" onAction="#onAddPart" prefHeight="25.0" prefWidth="27.0" text="+" />
         <GridPane fx:id="toolPane" alignment="CENTER" prefHeight="90.0" prefWidth="275.0">
           <columnConstraints>
               <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
               <Button mnemonicParsing="false" onAction="#onMoveLeft" text="LEFT" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               <Button mnemonicParsing="false" onAction="#onMoveRight" text="RIGHT" GridPane.columnIndex="3" GridPane.rowIndex="1" />
               <Button mnemonicParsing="false" onAction="#onMoveUp" text="UP" GridPane.columnIndex="2" />
               <Button mnemonicParsing="false" onAction="#onMoveDown" text="DOWN" GridPane.columnIndex="2" GridPane.rowIndex="2" />
               <Button mnemonicParsing="false" onAction="#onRotate" text="Rotate" GridPane.columnIndex="4" />
               <Button mnemonicParsing="false" onAction="#onFlip" text="Flip" GridPane.columnIndex="4" GridPane.rowIndex="2" />
               <Button mnemonicParsing="false" onAction="#onRemovePart" text="-" GridPane.rowIndex="2" />
            </children>
         </GridPane>
      </children>
   </HBox>
   <BorderPane layoutY="152.0" prefHeight="448.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="152.0">
      <center>
         <ScrollPane BorderPane.alignment="CENTER">
            <content>
               <FlowPane fx:id="solutionFlowPane" hgap="20.0" prefWrapLength="800.0" vgap="20.0" />
            </content>
            <BorderPane.margin>
               <Insets top="20.0" />
            </BorderPane.margin>
         </ScrollPane>
      </center>
      <top>
          <Canvas fx:id="mainCanvas" height="179.0" width="326.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </BorderPane.margin></Canvas>
      </top>
      <bottom>
         <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <items>
               <ProgressBar fx:id="solverProgressBar" prefWidth="200.0" progress="0.0" />
                 <Label fx:id="messageLabel" />
                 <Label fx:id="solutionLabel" />
            </items>
         </ToolBar>
      </bottom>
   </BorderPane>
</AnchorPane>
